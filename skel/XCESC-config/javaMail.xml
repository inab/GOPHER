<?xml version="1.0" encoding="UTF-8"?>

<!--
	javaMail properties needed to send e-mails
-->

<properties>
	<property name="mail.debug" value="false"/>
	<property name="mail.smtp.starttls.enable" value="true"/>
	<property name="mail.smtp.auth" value="true"/>
	<property name="mail.smtp.port" value="25"/>
	<property name="mail.smtp.host" value="webmail.cnio.es"/>
	<property name="mail.smtp.user" value="biodb"/>
	<property name="mail.smtp.password"  value="cnio-98"/>
</properties>
